<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.quan.appaddressingall.dao.AppInstanceDao">
    <select id="getDefaultAppInstance" resultType="com.quan.appaddressingall.entity.APIInstanceResult">
        select ai.*,am.app_secret from app_instance as ai join app_id_meta as am on am.app_id = ai.app_id  where ai.is_default;
    </select>
</mapper>