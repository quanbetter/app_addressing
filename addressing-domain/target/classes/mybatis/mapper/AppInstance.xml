<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.quan.addressing.dao.AppInstanceDao">

    <insert id="insertAppInstance" parameterType="com.quan.addressing.entity.AppInstanceEntity">
        insert into app_instance (app_id, address, port)
        values (#{appId},#{address},#{port})
    </insert>
</mapper>